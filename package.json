{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:less": "lessc --js app/styles/antd.less app/styles/antd.css",
    "build:css": "sass --no-source-map --style=compressed  app/:app/",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.js --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev": "npm-run-all -p dev:*",
    "dev:less": "lessc --js app/styles/antd.less > app/styles/antd.css",
    "dev:css": "npm run sass",
    "dev:worker": "esbuild ./app/entry.worker.js --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "start": "cross-env PORT=4264 remix-serve build",
    "sass": "sass --watch  app/:app/"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@tanstack/react-query": "^4.8.0",
    "@tanstack/react-query-devtools": "^4.8.0",
    "antd": "^4.23.4",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "currency-symbol-map": "^5.1.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "html-escaper": "^3.0.3",
    "i18next": "^21.9.2",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-backend": "^1.4.4",
    "isbot": "^3.6.0",
    "leaflet": "^1.9.1",
    "leaflet.markercluster": "^1.5.3",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-leaflet": "^4.1.0",
    "remix-i18next": "^4.1.1",
    "remix-utils": "^3.3.0",
    "string-strip-html": "^11.6.9"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  },
  "engines": {
    "node": ">=14"
  }
}
